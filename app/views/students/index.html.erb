<div class="row">
  <div class="col-s12">
    <h3 class="center-align">Students Directory</h3>
  </div>
</div>



<div class="row">
  <% @students.each do |student| %>
    <div class="col s6 m6">
      <div class="card small blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">Name: <%= student.name %></span>
          <span class="card-title">Email: <%= student.email %></span>
          <p>Course Enrollments: None</p>
        </div>
        <div class="card-action">
          <a href="#">Enroll</a>
          <a href="#">Info</a>
        </div>
      </div>
  </div>
  <% end %>
</div>
